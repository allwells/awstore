{
  "name": "awstore",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:store": "pnpm --filter store build",
    "build:store-studio": "pnpm --filter store-studio build",
    "build": "concurrently -n \"store,studio\" -c \"magenta.bold,red.bold\" \"pnpm run build:store\" \"pnpm run build:store-studio\"",
    "dev:store": "pnpm --filter store dev",
    "dev:store-studio": "pnpm --filter store-studio dev",
    "dev": "concurrently -n \"store,studio\" -c \"magenta.bold,red.bold\" \"pnpm run dev:store\" \"pnpm run dev:store-studio\""
  },
  "keywords": [],
  "author": "Allwell Onen",
  "license": "MIT",
  "packageManager": "pnpm@10.6.5",
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
